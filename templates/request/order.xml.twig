{% apply spaceless %}
<?xml version="1.0" encoding="windows-1252"?>

<EoCustomLinkStoreOrdersNormal Type="TEoCustomLinkStoreOrdersNormal" xsi:noNamespaceSchemaLocation="GdxEoStructures.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Data Type="TEoOrderMxList">
        <_TEoListBase_Items>
            <EoOrderMx Type="TEoOrderMx">
                {% if request.id is defined %}
                <OrderId Type="TEoKeyIntInfraMx">
                    <Id>{{ request.id|format_value }}</Id>
                </OrderId>
                {% endif %}
                {% if request.client_id is defined %}
                <ClientId Type="TEoKeyIntInfraMx">
                    <Id>{{ request.client_id|format_value }}</Id>
                </ClientId>
                {% endif %}
                {% if request.contact is defined %}
                <Contact>{{ request.contact|format_value }}</Contact>
                {% endif %}
                {% if request.moment is defined %}
                <Moment>{{ request.moment|format_value }}</Moment>
                {% endif %}
                <Notes></Notes>
                {% if request.product_id is defined %}
                <ProductId Type="TEoKeyIntInfraMx"> 
                    <Id>{{ request.product_id|format_value }}</Id>
                </ProductId>
                {% endif %}
                {% if request.product_id_automatic_articles is defined %}
                <ProductIdAutomaticArticles>{{ request.product_id_automatic_articles|format_value }}</ProductIdAutomaticArticles>
                {% endif %}
                {% if request.goods is defined %}
                <Goods Type="TEoGoodMxList">
                    <_TEoListBase_Items>
                        {% for good in request.goods %}
                        <EoGoodMx Type="TEoGoodMx">
                            {% if good.id is defined %}
                            <GoodId Type="TEoKeyIntInfraMx">
                                <Id>{{ good.id|format_value }}</Id>
                            </GoodId>
                            {% endif %}
                            {% if good.barcode is defined %}
                            <Barcode>{{ good.barcode|format_value }}</Barcode>
                            {% endif %}
                            {% if good.comments is defined %}
                            <Comments>{{ good.comments|format_value }}</Comments>
                            {% endif %}
                            {% if good.depth is defined %}
                            <Depth>{{ good.depth|format_value }}</Depth>
                            {% endif %}
                            {% if good.height is defined %}
                            <Height>{{ good.height|format_value }}</Height>
                            {% endif %}
                            {% if good.packing_type is defined %}
                            <Packing Type="TEoPackingMx">
                                <Name>{{ good.packing_type|format_value }}</Name>
                            </Packing>
                            {% endif %}
                            {% if good.parts is defined %}
                            <Parts>{{ good.parts|format_value }}</Parts>
                            {% endif %}
                            {% if good.volume is defined %}
                            <Volume>{{ good.volume|format_value }}</Volume>
                            {% endif %}
                            {% if good.volume_weight is defined %}
                            <VolumeWeight>{{ good.volume_weight|format_value }}</VolumeWeight>
                            {% endif %}
                            {% if good.article_weight is defined %}
                            <ArticleWeight>{{ good.article_weight|format_value }}</ArticleWeight>
                            {% endif %}
                            {% if good.weight is defined %}
                            <Weight>{{ good.weight|format_value }}</Weight>
                            {% endif %}
                            {% if good.width is defined %}
                            <Width>{{ good.width|format_value }}</Width>
                            {% endif %}
                        </EoGoodMx>
                        {% endfor %}
                    </_TEoListBase_Items>
                </Goods>
                {% endif %}
                {% if request.tasks is defined %}
                <Tasks Type="TEoTaskMxList">
                    <_TEoListBase_Items>
                        {% for task in request.tasks %}
                        <EoTaskMx Type="TEoTaskMx">
                            {% if task.id is defined %}
                            <TaskId Type="TEoKeyIntInfraMx">
                                <Id>{{ task.id|format_value }}</Id>
                            </TaskId>
                            {% endif %}
                            {% if task.address is defined %}
                            <Address Type="TEoAddress">
                                {% if task.address.name is defined %}
                                <Name>{{ task.address.name|format_value }}</Name>
                                {% endif %}
                                {% if task.address.premise is defined %}
                                <Premise>{{ task.address.premise|format_value }}</Premise>
                                {% endif %}
                                {% if task.address.street is defined %}
                                <Street>{{ task.address.street|format_value }}</Street>
                                {% endif %}
                                {% if task.address.number is defined %}
                                <Number>{{ task.address.number|format_value }}</Number>
                                {% endif %}
                                {% if task.address.postal_code is defined %}
                                <PostalCode>{{ task.address.postal_code|format_value }}</PostalCode>
                                {% endif %}
                                {% if task.address.place is defined %}
                                <Place>{{ task.address.place|format_value }}</Place>
                                {% endif %}
                                {% if task.address.state is defined %}
                                <State>{{ task.address.state|format_value }}</State>
                                {% endif %}
                                {% if task.address.country is defined %}
                                <Country>{{ task.address.country|format_value }}</Country>
                                {% endif %}
                                {% if task.address.country_code is defined %}
                                <CountryCode>{{ task.address.country_code|format_value }}</CountryCode>
                                {% endif %}
                            </Address>
                            {% endif %}
                            {% if task.cash_on_delivery is defined %}
                            <CashOnDelivery>{{ task.cash_on_delivery|format_value }}</CashOnDelivery>
                            {% endif %}
                            {% if task.connectivity is defined %}
                            <Connectivity Type="TEoConnectivity">
                                {% if task.connectivity.email is defined %}
                                <Email>{{ task.connectivity.email|format_value }}</Email>
                                {% endif %}
                                {% if task.connectivity.fax is defined %}
                                <Fax>{{ task.connectivity.fax|format_value }}</Fax>
                                {% endif %}
                                {% if task.connectivity.mobile is defined %}
                                <Mobile>{{ task.connectivity.mobile|format_value }}</Mobile>
                                {% endif %}
                                {% if task.connectivity.phone is defined %}
                                <Phone>{{ task.connectivity.phone|format_value }}</Phone>
                                {% endif %}
                                {% if task.connectivity.web is defined %}
                                <Web>{{ task.connectivity.web|format_value }}</Web>
                                {% endif %}
                            </Connectivity>
                            {% endif %}
                            {% if task.contact_name is defined %}
                            <ContactName>{{ task.contact_name|format_value }}</ContactName>
                            {% endif %}
                            {% if task.instructions is defined %}
                            <Instructions>{{ task.instructions|format_value }}</Instructions>
                            {% endif %}
                            {% if task.operator_id_automatic is defined %}
                            <OperatorIdAutomatic>{{ task.operator_id_automatic|format_value }}</OperatorIdAutomatic>
                            {% endif %}
                            {% if task.operator_id_subautomatic is defined %}
                            <OperatorIdSubAutomatic>{{ task.operator_id_subautomatic|format_value }}</OperatorIdSubAutomatic>
                            {% endif %}
                            {% if task.reference_our is defined %}
                            <ReferenceOur>{{ task.reference_our|format_value }}</ReferenceOur>
                            {% endif %}
                            {% if task.reference_your is defined %}
                            <ReferenceYour>{{ task.reference_your|format_value }}</ReferenceYour>
                            {% endif %}
                            {% if task.requested is defined %}
                            <Requested Type="TEoDateTimeWindow">
                                {% if task.requested.date_time_end is defined %}
                                <DateTimeEnd>{{ task.requested.date_time_end|format_value }}</DateTimeEnd>
                                {% endif %}
                                {% if task.requested.date_time_begin is defined %}
                                <DateTimeBegin>{{ task.requested.date_time_begin|format_value }}</DateTimeBegin>
                                {% endif %}
                            </Requested>
                            {% endif %}
                            {% if task.task_type_id is defined %}
                            <TaskTypeId Type="TEoKeyIntInfraMx">
                                <Id>{{ task.task_type_id|format_value }}</Id>
                            </TaskTypeId>
                            {% endif %}
                        </EoTaskMx>
                        {% endfor %}
                    </_TEoListBase_Items>
                </Tasks>
                {% endif %}
                {% if request.goods_to_tasks is defined %}
                <GoodsToTasks Type="TEoGoodToTaskMxList">
                    <_TEoListBase_Items>
                        {% for good_to_tasks in request.goods_to_tasks %}
                        <EoGoodToTaskMx Type="TEoGoodToTaskMx">
                            {% if good_to_tasks.good_id is defined %}
                            <GoodId Type="TEoKeyIntInfraMx">
                                <Id>{{ good_to_tasks.good_id|format_value }}</Id>
                            </GoodId>
                            {% endif %}
                            {% if good_to_tasks.task_id is defined %}
                            <TaskId Type="TEoKeyIntInfraMx">
                                <Id>{{ good_to_tasks.task_id|format_value }}</Id>
                            </TaskId>
                            {% endif %}
                        </EoGoodToTaskMx>
                        {% endfor %}
                    </_TEoListBase_Items>
                </GoodsToTasks>
                {% endif %}
                {% if request.external_done is defined %}
                <ExternalDone>{{ request.external_done|format_value }}</ExternalDone>
                {% endif %}
                {% if request.external_source is defined %}
                <ExternalSource>{{ request.external_source|format_value }}</ExternalSource>
                {% endif %}
            </EoOrderMx>
        </_TEoListBase_Items>
    </Data>
</EoCustomLinkStoreOrdersNormal>
{% endapply %}